from pymongo import MongoClient
client = MongoClient()
db = client.wdj
documentname = db.useapkChangeapk

keys = {}
for k in documentname.find():
    key = k['apk']
    if keys.has_key(key):
        print 'duplicate key %s' % key
        documentname.remove({'_id':k['_id']})
    else:
        print 'first record key %s' % key
        keys[key]=1




